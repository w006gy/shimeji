		<Action Name="PickSanBaoAction" Type="Embedded" Class="com.group_finity.mascot.action.Interact" BorderType="Floor">
			<Animation>
				<Pose Image="/M_run_11.png" ImageAnchor="64,136" Velocity="0,0" Duration="8" />
				<Pose Image="/M_run_10.png" ImageAnchor="64,136" Velocity="0,0" Duration="8" />
