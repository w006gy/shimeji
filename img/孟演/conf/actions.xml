		<Action Name="WalkAndThrowIEFromLeft" Type="Sequence" Loop="false">
			<ActionReference Name="Jumping" TargetX="${mascot.environment.activeIE.right}" TargetY="${mascot.environment.activeIE.bottom+64}" />
