		<Action Name="PickSanBao" Type="Sequence">
			<!-- <ActionReference Name="PickSanBaoAction" /> -->
			<ActionReference Name="ActionWithSanBao" />
			<!-- {TargetY &lt; mascot.anchor.y} -->
			<ActionReference Name="Look" LookRight="${mascot.anchor.x &lt; (mascot.environment.workArea.left + mascot.environment.workArea.width) / 2}" />
			<ActionReference Name="MoveSanBao" Duration="160" />
			<!-- TargetX="${(mascot.environment.workArea.left + mascot.environment.workArea.width) / 2}"  -->
		</Action>

