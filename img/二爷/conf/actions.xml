		<!-- Falling -->

		<Action Name="Jumping" Type="Embedded" Class="com.group_finity.mascot.action.Jump"
			VelocityParam="15">
			<Animation>
				<Pose Image="/bandit04_simple_01.png" ImageAnchor="64,136" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="Falling" Type="Embedded" Class="com.group_finity.mascot.action.Fall"
			RegistanceX="0.05" RegistanceY="0.1" Gravity="2.0">
			<Animation>
				<Pose Image="/bandit04_simple_01.png" ImageAnchor="64,136" Velocity="0,0" Duration="250" />
			</Animation>
		</Action>

		<Action Name="Bouncing" Type="Animate" BorderType="Floor">
			<Animation>
				<Pose Image="/bandit04_simple_01.png" ImageAnchor="64,136" Velocity="0,0" Duration="5" />
			</Animation>
		</Action>

		<Action Name="Tripping" Type="Animate" BorderType="Floor">
