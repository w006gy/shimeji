				<Pose Image="/bandit04_walk_07.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_08.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
			</Animation>
		</Action>

		<Action Name="RunAndHit2" Type="Embedded" Class="com.group_finity.mascot.action.ScanMove" Affordance="HitDown" Behavior="PunchInteract" TargetBehavior="BePunched" BorderType="Floor">
			<Animation>
				<Pose Image="/bandit04_walk_01.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_02.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_03.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_04.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_05.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_06.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_07.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
				<Pose Image="/bandit04_walk_08.png" ImageAnchor="64,136" Velocity="-5,0" Duration="2" />
			</Animation>
		</Action>

		<Action Name="RunAndBeHit" Type="Embedded" Class="com.group_finity.mascot.action.ScanMove" Affordance="HitDown" Behavior="BePunched" TargetBehavior="PunchInteract" BorderType="Floor">
			<Animation>
				<Pose Image="/bandit04_walk_01.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
				<Pose Image="/bandit04_walk_02.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
				<Pose Image="/bandit04_walk_03.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
				<Pose Image="/bandit04_walk_04.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
				<Pose Image="/bandit04_walk_05.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
				<Pose Image="/bandit04_walk_06.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
				<Pose Image="/bandit04_walk_07.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
				<Pose Image="/bandit04_walk_08.png" ImageAnchor="64,136" Velocity="-3,0" Duration="2" />
			</Animation>
		</Action>

		<Action Name="HuntBePunch" Type="Sequence">
			<ActionReference Name="Stand" Duration="5" /> 
			<!-- a small delay is needed to prevent an infinite loop -->
			<ActionReference Name="RunAndBeHit" />
		</Action>

		<Action Name="HuntPunchDefend2" Type="Sequence">
			<ActionReference Name="Stand" Duration="5" /> 
			<!-- a small delay is needed to prevent an infinite loop -->
			<ActionReference Name="RunAndPunch2" />
		</Action>

		<Action Name="HuntPunch2" Type="Sequence">
			<ActionReference Name="Stand" Duration="5" /> 
			<ActionReference Name="RunAndHit2" />
		</Action>

		<Action Name="HuntJumpPunch2" Type="Sequence">
			<ActionReference Name="Stand" Duration="5" /> 
			<ActionReference Name="RunAndJumpPunch2" />
		</Action>

